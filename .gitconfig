[user]
	name = moeabdol
	email = admin.r99@gmail.com
[core]
	excludesfile = ~/.gitignore
	editor = /usr/local/bin/vim
[alias]
	co = checkout
    s = status
    l = log
    d = diff
    g = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n'' %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
[push]
	default = matching
