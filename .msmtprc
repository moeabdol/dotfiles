defaults
auth on
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile ~/.config/msmtp/msmtp.log

# mohd.a.saed@gmail.com
account mohd.a.saed@gmail.com
host smtp.gmail.com
port 587
tls_starttls on
from "Mohammed Saed"
user "mohd.a.saed@gmail.com"
passwordeval "pass mohd.a.saed@gmail.com"

