+SELECT
+	products.id,
+	products.barcode,
+	products.name_en,
+	products.name_ar,
+	products.active,
+	json_strip_nulls(
+		json_build_object(
+			'id', countries.id,
+			'nameEn', countries.name_en,
+			'nameAr', countries.name_ar,
+			'code', countries.code
+		)
+	) AS country_of_origin,
+	json_strip_nulls(
+		json_build_object(
+			'id', sale_units.id,
+			'nameEn', sale_units.name_en,
+			'nameAr', sale_units.name_ar
+		)
+	) AS sale_unit,
+	json_strip_nulls(
+		json_build_object(
+			'id', weight_units.id,
+			'nameEn', weight_units.name_en,
+			'nameAr', weight_units.name_ar
+		)
+	) AS weight_unit,
+	json_agg(
+		json_strip_nulls(
+			json_build_object(
+				'supplyId', supplies.id,
+				'shopId', supplies.id,
+				'quantity', supply_items.quantity
+			)
+		)
+	) AS quantities
+	supply_items.created_at,
+	supply_items.created_by,
+	supply_items.updated_at,
+	supply_items.updated_by
+FROM products AS p
+	INNER JOIN supply_items ON supply_items.product_id=p.id
+	INNER JOIN supplies ON supplies.id=supply_items.supply_id
+	INNER JOIN shops ON shops.id=supplies.shop_id
+	LEFT JOIN product_images ON product_images.product_id=supply_items.product_id AND product_images.sequence=0
+	LEFT JOIN product_classes ON product_classes.id=products.class_id
+	LEFT JOIN product_subclasses ON product_subclasses.id=products.subclass_id
+	LEFT JOIN product_categories ON categories.id=products.category_id
+	LEFT JOIN product_subcategories ON product_subcategories.id=products.subcategory_id
+	LEFT JOIN product_types ON product_types.id=products.type_id
+	LEFT JOIN product_subtypes ON product_subtypes.id=products.subtype_id
+	LEFT JOIN product_kinds ON product_kinds.id=products.kind_id
+	INNER JOIN countries ON countries.id=products.country_of_origin_id
+	INNER JOIN sale_units ON sale_units.id=products.sale_unit_id
+	LEFT JOIN weight_units ON weight_units.id=products.weight_unit_id
+WHERE
+	s.shop_id=ANY('')
+--  GROUP BY p.id, pi.url, cnt.id, su.id, wu.id
+ORDER BY
+	CASE WHEN @orderBy::varchar='createdAt' AND @orderType::varchar='asc' THEN supply_items.created_at END ASC,
+	CASE WHEN @orderBy='createdAt'          AND @orderType='desc'         THEN supply_items.created_at END DESC,
+	CASE WHEN @orderBy='updatedAt'          AND @orderType='asc'          THEN supply_items.updated_at END ASC,
+	CASE WHEN @orderBy='updatedAt'          AND @orderType='desc'         THEN supply_items.updated_at END DESC,
+	CASE WHEN @orderBy='nameEn'             AND @orderType='asc'          THEN products.name_en END ASC,
+	CASE WHEN @orderBy='nameEn'             AND @orderType='desc'         THEN products.name_en END DESC,
+	CASE WHEN @orderBy='nameAr'             AND @orderType='asc'          THEN products.name_ar END ASC,
+	CASE WHEN @orderBy='nameAr'             AND @orderType='desc'         THEN products.name_ar END DESC
+OFFSET 0 LIMIT 1000;

