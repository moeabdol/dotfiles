super + {Up,Down,Right}
	$HOME/repos/bash-scripts/change-volume {up,down,mute}

XF86AudioRaiseVolume
	$HOME/repos/bash-scripts/change-volume up

XF86AudioLowerVolume
	$HOME/repos/bash-scripts/change-volume down

super + c
	rofi -show calc -no-show-match -no-sort

super + p
	alacritty --title pydoro -e /home/moeabdol/.local/bin/pydoro -e
